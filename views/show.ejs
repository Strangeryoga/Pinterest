<% include ./partials/header.ejs %>

<div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
    <div class="cards flex flex-wrap gap-10">

        <% user.posts.forEach(function(e) { %>
            <div class="card w-32  ">
                <div class="w-full bg-zinc-800 rounded-md flex items-start">
                    <img class="rounded-lg" src="/images/uploads/<%= e.image %>" alt="">
                </div>
                <h class="mt-2 text-sm"><%= e.title %> </h>
            </div>
        <% }) %>

       

    </div>
</div>

<% include ./partials/footer.ejs %>
